<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="beandiff" xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
    
    <bannerLeft>
        <name>BeanDiff</name>
        <href>http://www.beandiff.org</href>
    </bannerLeft>

	<skin>
	    <groupId>org.apache.maven.skins</groupId>
	    <artifactId>maven-fluido-skin</artifactId>
	    <version>1.3.0</version>
	</skin>
	<custom>
	    <fluidoSkin>
	    	<twitter>
        		<user>tkaczmarzyk</user>
        		<showUser>false</showUser>
        		<showFollowers>true</showFollowers>
      		</twitter>
	    	<topBarEnabled>false</topBarEnabled>
	        <sideBarEnabled>true</sideBarEnabled>
	    </fluidoSkin>
	</custom>

    <body>
    	<head>
    		<script type="text/javascript">
                $(document).ready(function () {
                    $("pre code").addClass("prettyprint");
                    prettyPrint();
                });
			</script>
    	</head>
    
        <links>
            <item name="source control" href="http://github.com/tkaczmarzyk/beandiff"/>
        </links>

        <menu name="Info &amp; guides">
            <item name="Home" href="index.html"/>
			<!--<item name="FAQ" href="faq.html"/>-->
        </menu>

        <menu ref="reports"/>
    </body>
</project>
